{
    "require": {
        "league/commonmark": "~0.10.0",
        "php": ">=5.3.0",
        "webuni/commonmark-attributes-extension": "~0.2.0",
        "webuni/commonmark-table-extension": "~0.3.0",
        "sensiolabs/security-checker": "~2.0",
        "gitonomy/gitlib": "~0.1.7"
    },
    "config": {
        "autoloader-suffix": "ComposerManager",
        "vendor-dir": "docroot/../vendor"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\CommonMark\\": [
                "docroot/sites/all/modules/contrib/commonmark/src"
            ],
            "Drupal\\GitClone\\": [
                "docroot/sites/all/modules/contrib/git_clone/src"
            ]
        }
    },
    "require-dev": {
        "drush/drush": "^8"
    }
}
